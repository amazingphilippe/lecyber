@incollection{vial_voir_2016,
	series = {Perceptions},
	title = {Voir et percevoir à l'ère numérique},
	url = {https://hal.archives-ouvertes.fr/hal-01516807},
	abstract = {Qu’est-ce qui se renverse et se bouleverse, se réforme et se transforme, se déplace et se remplace, dans ce que l'on appelle la « révolution numérique » ? En un mot : de quoi la révolution numérique est-elle la révolution ? La réponse peut tenir en une phrase : du point de vue phénoménologique, ce qui se renverse et se transforme, ce dont nous procédons au remplacement, ce sont nos struc- tures perceptives. Telle est l'hypothèse que nous mettons au travail. À travers l’adoption au plan socio-historique d’un nouveau système technique (le système technique numérique), nous adoptons au plan phénoménologique une nouvelle matrice ontophanique (l’ontophanie numérique). Comme toute révolution technique, la révolution numérique doit donc être interprétée comme une révolution ontophanique, c’est-à-dire un ébranle- ment du processus par lequel l’être nous apparaît et, par suite, un bouleversement de l’idée même que nous nous faisons de ce qui est réel.},
	urldate = {2020-02-05},
	booktitle = {Vivre par(mi) les écrans},
	publisher = {Les Presses du Réel},
	author = {Vial, Stéphane},
	month = 10,
	year = {2016},
	keywords = {ontophanie, phénoménotechnique, sentiment ontophanique, théorie de l'ontophanie},
	pages = {63--85},
	file = {HAL PDF Full Text:/Users/philippecaron/Zotero/storage/39CJS2GP/Vial - 2016 - Voir et percevoir à l'ère numérique.pdf:application/pdf}
}

